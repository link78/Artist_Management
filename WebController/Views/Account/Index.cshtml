@model IEnumerable<AppUser>

<div class="bg-primary m-1 p-1">
    
        <h4>User Accounts</h4>   
</div>

<table class="table table-striped table-bordered">
    <tr>
        <th>ID</th>
        <th>UserName</th>
        <th>Email</th>
    </tr>
    
        @foreach(var user in Model) {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
            </tr>
         }
    
</table>



